
<%- include('header'); -%>
<body>
    <div class="box" id="heading">
        <h1>
            <%= listName %>
        </h1>
    </div>

    <div class="box">

        <% for(var i=0; i<newListItem.length; i++) { %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= newListItem[i] %>
                </p>
            </div>

        <% } %>
        <form class="item" action="/" method="Post">
            <input name="newItem" pableholder="New Item" autocomplete="off">
            <button type="submit" name="button" value="<%= listType %>">+</button>
        </form>
    </div>



</body>

<%- include('footer'); -%>